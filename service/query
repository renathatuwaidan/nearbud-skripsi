--insert dummy ke review table
DO $$
DECLARE
    i INTEGER;
    reviewer TEXT;
    reviewee TEXT;
    rating INTEGER;
    review_text TEXT;
    user_prefix TEXT := 'U';
    community_prefix TEXT := 'C';
    sample_reviews TEXT[] := ARRAY[
        'Sangat bagus!',
        'Kurang memuaskan.',
        'Pelayanan oke.',
        'Komunitasnya aktif.',
        'Ramah dan membantu.',
        'Cukup baik.',
        'Biasa saja.',
        'Sangat direkomendasikan!',
        'Kurang responsif.',
        'Luar biasa!'
    ];
BEGIN
    FOR i IN 1..70 LOOP
        reviewer := user_prefix || LPAD(FLOOR(RANDOM() * 17 + 1)::TEXT, 3, '0');
        IF RANDOM() < 0.5 THEN
            reviewee := user_prefix || LPAD(FLOOR(RANDOM() * 17 + 1)::TEXT, 3, '0');
        ELSE
            reviewee := community_prefix || LPAD(FLOOR(RANDOM() * 19 + 1)::TEXT, 3, '0');
        END IF;

        rating := FLOOR(RANDOM() * 5 + 1);
        review_text := sample_reviews[FLOOR(RANDOM() * array_length(sample_reviews, 1) + 1)];

        -- Insert data
        INSERT INTO review (id_reviewee, id_reviewer, rating, review)
        VALUES (reviewee, reviewer, rating, review_text);
    END LOOP;
END $$;

